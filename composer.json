{
    "name": "elixir/elixir-security",
    "description": "Component security",
    "version": "1.0.0",
    "license": "BSD-3-Clause",
    "keywords": [
        "elixir",
        "security",
        "firewall",
        "rbac"
    ],
    "homepage": "https://github.com/cedtanghe/elixir-security",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/cedtanghe/elixir-http"
        },
        {
            "type": "vcs",
            "url": "https://github.com/cedtanghe/elixir-session"
        },
        {
            "type": "vcs",
            "url": "https://github.com/cedtanghe/elixir-stdlib"
        },
        {
            "type": "vcs",
            "url": "https://github.com/cedtanghe/elixir-config"
        }
    ],
    "require": {
        "php": ">=5.4",
        "elixir/elixir-stdlib": "*"
    },
    "require-dev": {
        "phpunit/PHPUnit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Elixir\\Security\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elixir\\Test\\Security\\": "tests/"
        }
    },
    "suggest": {
        "elixir/elixir-http": "*",
        "elixir/elixir-session": "*",
        "elixir/elixir-config": "*",
        "ircmaxell/password-compat": "php version < 5.5"
    }
}
